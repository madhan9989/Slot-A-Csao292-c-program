#include<stdio.h>
int main()
{
int a[50],i,n,large,small;
scanf("%d",&n);
for(i=0;i<n;i++)
scanf("%d",&a[i]);
large=a[0];
small=a[0];
for(i=0;i<n;i++)
if(a[i]>large)
large=a[i];
for(i=0;i,n;i++)
if(a[i]<small)
small=a[i];
printf("the largest elements is %d\n",large);
printf("the smallest elements is %d\n",small);
return 0;
}
